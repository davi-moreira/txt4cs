@article{batista_who_2018,
	title = {Who {Gets} {What} and {How} {Does} {It} {Matter}? {Importance}-{Weighted} {Portfolio} {Allocation} and {Coalition} {Support} in {Brazil}},
	volume = {10},
	issn = {1868-4890},
	shorttitle = {Who {Gets} {What} and {How} {Does} {It} {Matter}?},
	url = {https://journals.sub.uni-hamburg.de/giga/jpla/article/view/1143},
	abstract = {Who gets what in portfolio allocation, and how does it matter to coalition partners’ legislative support in presidential systems? I propose that portfolios are not all alike, and that their allocation as well as the support for the president’s agenda depends on the particular distribution of assets within the executive. The portfolio share allocated to coalition parties is weighted by a measure of importance based on the assets controlled by the ministry in question, such as policies, offices, and budgets. Once the weighted allocation of ministries has been identified, the results show that: 1) the president concentrates the most important ministries in their own party, mainly considering the policy dimension; 2) the positive relationship between portfolio allocation and legislative support remains, with the importance of specific dimensions being considered; and, 3) coalition partners do not respond differently in terms of legislative support in light of the different assets’ distribution within the portfolio allocation.},
	number = {3},
	journal = {Journal of Politics in Latin America},
	author = {Batista, Mariana},
	month = dec,
	year = {2018},
	pages = {99--134},
	file = {1143-1181-1-PB.pdf:/Users/davi/Google Drive/Academico/Biblioteca/1143-1181-1-PB.pdf:application/pdf;Who Gets What and How Does It Matter? Importance-Weighted Portfolio Allocation and Coalition Support in Brazil | Batista | Journal of Politics in Latin America:/Users/davi/Zotero/storage/Q8NMT79R/1143.html:text/html}
}

