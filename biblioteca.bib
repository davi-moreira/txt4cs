	zmnumber = {1149.62089},
	keywords = {instrumental variables, Average causal effect, causal effects, complier average causal effect, noncompliance, observational studies, propensity scores, randomized experiments, Rubin Causal Model},
	pages = {808--840},
	file = {euclid.aoas.1223908042.pdf:/Users/davi/Google Drive/Academico/Biblioteca/euclid.aoas.1223908042.pdf:application/pdf;Full Text PDF:/Users/davi/Zotero/storage/X6CC93T3/Rubin - 2008 - For objective causal inference, design trumps anal.pdf:application/pdf;Snapshot:/Users/davi/Zotero/storage/ZR7D7L22/1223908042.html:text/html}
