@article{stokes_spatial_1963,
	title = {Spatial {Models} of {Party} {Competition}},
	volume = {57},
	issn = {0003-0554, 1537-5943},
	url = {https://www.cambridge.org/core/journals/american-political-science-review/article/spatial-models-of-party-competition/E35333E50CCC6DD0CCB24000626BFBA8},
	doi = {10.2307/1952828},
	abstract = {The use of spatial ideas to interpret party competition is a universal phenomenon of modern politics. Such ideas are the common coin of political journalists and have extraordinary influence in the thought of political activists. Especially widespread is the conception of a liberal-conservative dimension on which parties maneuver for the support of a public that is itself distributed from left to right. This conception goes back at least to French revolutionary times and has recently gained new interest for an academic audience through its ingenious formalization by Downs and others. However, most spatial interpretations of party competition have a very poor fit with the evidence about how large-scale electorates and political leaders actually respond to politics. Indeed, the findings on this point are clear enough so that spatial ideas about party competition ought to be modified by empirical observation. I will review here evidence that the “space” in which American parties contend for electoral support is very unlike a single ideological dimension, and I will offer some suggestions toward revision of the prevailing spatial model.},
	language = {en},
	number = {2},
	urldate = {2019-05-05},
	journal = {American Political Science Review},
	author = {Stokes, Donald E.},
	month = jun,
	year = {1963},
	pages = {368--377},
	file = {Snapshot:/Users/davi/Zotero/storage/VAMC3F3Q/E35333E50CCC6DD0CCB24000626BFBA8.html:text/html;stokes1963.pdf:/Users/davi/Google Drive/Academico/Biblioteca/stokes1963.pdf:application/pdf}
}

@phdthesis{salles_competicao_2019,
	address = {Rio de Janeiro},
	title = {Competição eleitoral no {Brasil}: uma perspectiva programática},
	abstract = {O objetivo desta tese é investigar a competição programática no Brasil através da análise
das plataformas de governo registradas pelos candidatos ao executivo municipal em 2012 e em
2016. O método adotado é o modelo automatizado de escalonamento, wordfish, que atribui
posições aos documentos a partir das palavras que utilizam. Porque, apesar do paradoxo
programático imposto pelo clássico modelo downsiano, é possível encontrar na literatura
incentivos para a divergência, a hipótese principal deste trabalho postula que a competição
eleitoral no Brasil também se estrutura programaticamente. Para que seja corroborada, as
propostas apresentadas pelos concorrentes em uma mesma disputa devem diferenciar-se entre
si. A hipótese secundária, por sua vez, aponta que essa competição programática se alinha
partidariamente e será corroborada se uma mesma agremiação apresentar posições semelhantes
no espectro programático ao longo de diferentes contextos. Os dois testes incluem a análise de
cada um dos corpus em sua totalidade – somando 13.649 programas de 4.970 municípios, em
2012, e 15.742 documentos de 5.358 localidades, em 2016 – e também a partir de cinco
variáveis de controle: (1) região, (2) tamanho e (3) desenvolvimento do município, (4)
analfabetos e (5) população rural. Os resultados mostram que há competição programática no
Brasil, apesar de nem sempre ela se alinhar partidariamente},
	school = {Instituto de Estudos Sociais e Políticos, Universidade do Estado do Rio de Janeiro},
	author = {Salles, Nara},
	year = {2019},
	file = {SALLES, Nara_Competicao eleitoral no Brasil uma perspectiva programatica.pdf:/Users/davi/Google Drive/Academico/Biblioteca/SALLES, Nara_Competicao eleitoral no Brasil uma perspectiva programatica.pdf:application/pdf}
}

@article{slapin_scaling_2008,
