	file = {ScienceDirect Snapshot:/Users/davi/Zotero/storage/3KZZI9YC/S0261379416302104.html:text/html}
}

@article{samuels_power_nodate,
	title = {The {Power} of {Partisanship} in {Brazil}: {Evidence} from {Survey} {Experiments}},
